import random


for num in range(1, 20):
    a = random.randint(1,20)
    b = random.randint(1,20)
    if num % 2 != 0 :
        print("{0} + {1} = ".format(a, b))
    else:
        if a > b:
            print("{0} - {1} = ".format(a, b))
        else:
            print("{0} - {1} = ".format(b, a))
